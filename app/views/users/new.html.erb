<%= form_for @user do  |f| %>
 Name: <%= f.text_field :name %>
 Height: <%= f.text_field :height %> <br>
 

 <%= f.label :password %> <br>
  <%= f.password_field :password %> <br>

 Nausea: <%=f.number_field :nausea %>
 Happiness: <%=f.number_field :happiness %>
 Tickets: <%= f.number_field :tickets %>
    
 <%= f.submit %>
 <% end %>